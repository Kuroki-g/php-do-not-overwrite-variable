{
	"name": "php-do-not-overwrite-variable",
	"displayName": "PHP DO NOT OVERWRITE VARIABLE",
	"description": "For everyone who do not want overwrite variable!",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": ["Other"],
	"activationEvents": [],
	"main": "./client/out/extension",
	"contributes": {
		"commands": [
			{
				"command": "php-do-not-overwrite-variable.analyzeActivatedFile",
				"title": "[php-do-not-overwrite-variable] Analyze current activated php file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint . --ext .ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "22.x",
		"@types/vscode": "^1.96.0",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.16.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"php-parser": "^3.2.2",
		"vscode-languageserver": "^9.0.1",
		"vscode-languageserver-textdocument": "^1.0.12"
	}
}
